        "typescript": "2.4.2",
        "zone.js": "^0.8.14"
    },
    "repository": "https://github.com/docsource/angular-tree-component.git",
    "bugs": {
        "url": "https://github.com/docsource/angular-tree-component/issues"
    },
    "engines": {
        "node": ">= 4.2.1",
        "npm": ">= 3"
    }
}
